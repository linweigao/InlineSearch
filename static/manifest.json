{
    "manifest_version": 2,
    "name": "Inline Search",
    "description": "Direct upload/download! Support DM! Mini video player! Keyboard navigation and more.",
    "version": "1.0.23",
    "browser_action": {
        "default_icon": "instagram-48.png"
    },
    "icons": {
        "16": "instagram-16.png",
        "48": "instagram-48.png",
        "128": "instagram-128.png"
    },
    "content_scripts": [
        {
            "run_at": "document_start",
            "js": [
                "js/content.js"
            ],
            "css": [
                "content.css"
            ],
            "matches": [
                "*://www.instagram.com/*"
            ]
        }
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "permissions": [
        "contextMenus"
    ]
}
